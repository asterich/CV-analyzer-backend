{
	"openapi": "3.0.0",
	"info": {
		"title": "CV API",
		"version": "1.0.0"
	},
	"servers": [
		{
			"url": "http://localhost:8080/api/v1",
			"description": "Development server"
		}
	],
	"paths": {
		"/cv": {
			"get": {
				"summary": "获取CV,支持单靠ID和Filename搜索(返回单个CV)和靠degree,workingYears(返回多个CV),age取交集搜索以及在所有参数为空时获取所有CV(返回多个CV)",
				"parameters": [
					{
						"in": "query",
						"name": "id",
						"required": false,
						"schema": {
							"type": "string"
						},
						"description": "The id of the CV."
					},
					{
						"in": "query",
						"name": "filename",
						"required": false,
						"schema": {
							"type": "string"
						},
						"description": "The filename of the CV."
					},
					{
						"in": "query",
						"name": "name",
						"required": false,
						"schema": {
							"type": "string"
						},
						"description": "The name to search for CVs."
					},
					{
						"in": "query",
						"name": "degree",
						"required": true,
						"schema": {
							"type": "string"
						},
						"description": "The degree to search for CVs"
					},
					{
						"in": "query",
						"name": "workingYears",
						"required": true,
						"schema": {
							"type": "integer"
						},
						"description": "The minimum number of working years to search for CVs."
					},
					{
						"in": "query",
						"name": "age",
						"required": true,
						"schema": {
							"type": "integer"
						},
						"description": "The maxmum number of age to search for CVs."
					},
					{
						"in": "query",
						"name": "pagesize",
						"required": false,
						"schema": {
							"type": "string"
						},
						"description": "The number of CVs to return."
					},
					{
						"in": "query",
						"name": "pagenum",
						"required": false,
						"schema": {
							"type": "string"
						},
						"description": "The page number of CVs to return."
					}
				],
				"responses": {
					"200": {
						"description": "A CV object or CVs Object",
						"content": {
							"application/json": {
								"schema": {
									"properties": {
										"type": "array",
										"items": {
											"$ref": "#/components/schemas/CV"
										}
									},
									"additionalProperties": {
										"$ref": "#/components/schemas/CV"
									}
								}
							}
						}
					},
					"404": {
						"description": "CV not found"
					},
					"500": {
						"description": "Internal server error"
					}
				}
			}
		},
		"/cv/id": {
			"delete": {
				"summary": "通过ID删除CV",
				"parameters": [
					{
						"in": "path",
						"name": "id",
						"required": true,
						"schema": {
							"type": "string"
						},
						"description": "The id of the CV to delete."
					}
				],
				"responses": {
					"200": {
						"description": "CV successfully deleted"
					},
					"404": {
						"description": "CV not found"
					},
					"500": {
						"description": "Internal server error"
					}
				}
			}
		},
		"/cv/filename": {
			"delete": {
				"summary": "通过文件名删除CV",
				"parameters": [
					{
						"in": "path",
						"name": "filename",
						"required": true,
						"schema": {
							"type": "string"
						},
						"description": "The filename of the CV to delete."
					}
				],
				"responses": {
					"200": {
						"description": "CV successfully deleted"
					},
					"500": {
						"description": "Internal server error"
					}
				}
			}
		},
		"/cv/upload": {
			"post": {
				"summary": "上传CV",
				"requestBody": {
					"content": {
						"multipart/form-data": {
							"schema": {
								"type": "object",
								"properties": {
									"file": {
										"type": "string",
										"format": "binary"
									}
								}
							}
						}
					}
				},
				"responses": {
					"200": {
						"description": "CV successfully uploaded"
					},
					"500": {
						"description": "Internal server error"
					}
				}
			}
		},
		"/positions": {
			"get": {
				"summary": "通过不同的关键词(name,degree,major,workingyears)获取positions,没有关键词则获取所有positions",
				"parameters": [
					{
						"in": "query",
						"name": "pagesize",
						"required": true,
						"schema": {
							"type": "string"
						},
						"description": "The number of CVs to return."
					},
					{
						"in": "query",
						"name": "pagenum",
						"required": true,
						"schema": {
							"type": "string"
						},
						"description": "The page number of CVs to return."
					},
					{
						"in": "query",
						"name": "name",
						"required": true,
						"schema": {
							"type": "string"
						}
					},
					{
						"in": "query",
						"name": "major",
						"required": true,
						"schema": {
							"type": "string"
						}
					},
					{
						"in": "query",
						"name": "degree",
						"required": true,
						"schema": {
							"type": "string"
						}
					},
					{
						"in": "query",
						"name": "working_years",
						"required": true,
						"schema": {
							"type": "integer"
						}
					}
				],
				"responses": {
					"200": {
						"description": "An array of positions",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/Position"
								}
							}
						}
					},
					"500": {
						"description": "Internal server error"
					}
				}
			}
		},
		"/positions/id": {
			"delete": {
				"summary": "通过positionID删除position",
				"parameters": [
					{
						"in": "path",
						"name": "id",
						"required": true,
						"schema": {
							"type": "string"
						}
					}
				],
				"responses": {
					"200": {
						"description": "Position successfully deleted"
					},
					"500": {
						"description": "Internal server error"
					}
				}
			}
		},
		"/count_degree": {
			"get": {
				"summary": "获取不同学历的数量",
				"parameters": [
					{
						"in": "query",
						"name": "pagesize",
						"required": true,
						"schema": {
							"type": "string"
						},
						"description": "The number of degrees that would be counted to return."
					}
				],
				"responses": {
					"200": {
						"description": "An map of degrees",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/Degree"
								}
							}
						}
					},
					"500": {
						"description": "Internal server error"
					}
				}
			}
		},
		"/count_workingyears": {
			"get": {
				"summary": "获取不同工作年份的数量",
				"parameters": [
					{
						"in": "query",
						"name": "pagesize",
						"required": true,
						"schema": {
							"type": "string"
						},
						"description": "The number of workingyears that would be counted to return."
					}
				],
				"responses": {
					"200": {
						"description": "An map of workingyears",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/Workingyears"
								}
							}
						}
					},
					"500": {
						"description": "Internal server error"
					}
				}
			}
		}
	},
	"components": {
		"schemas": {
			"CV": {
				"type": "object",
				"properties": {
					"code": {
						"type": "integer"
					},
					"msg": {
						"type": "string"
					},
					"data": {
						"type": "array",
						"items": {
							"type": "object",
							"additionalProperties": true
						}
					}
				}
			},
			"Degree": {
				"type": "object",
				"properties": {
					"code": {
						"type": "integer"
					},
					"msg": {
						"type": "string"
					},
					"data": {
						"type": "object",
						"properties": {
							"mapData": {
								"type": "object",
								"additionalProperties": {
									"type": "string"
								}
							}
						}
					}
				}
			},
			"Workingyears": {
				"type": "object",
				"properties": {
					"code": {
						"type": "integer"
					},
					"msg": {
						"type": "string"
					},
					"data": {
						"type": "object",
						"properties": {
							"mapData": {
								"type": "object",
								"additionalProperties": {
									"type": "int"
								}
							}
						}
					}
				}
			},
			"Position": {
				"type": "object",
				"properties": {
					"code": {
						"type": "integer"
					},
					"msg": {
						"type": "string"
					},
					"data": {
						"type": "array",
						"items": {
							"type": "object",
							"additionalProperties": true
						}
					}
				}
			}
		}
	}
}
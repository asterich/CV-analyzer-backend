	"paths": {
		"/cv/id":{
			"get": {
				"summary": "Retrieve a CV by id",
				"parameters": [
					{
						"in": "query",
						"name": "id",
						"required": true,
						"schema": {
							"type": "string"
						},
						"description": "The id of the CV."
					}
				],
				"responses": {
					"200": {
						"description": "A CV object",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/CV"
								}
							}
						}
					},
					"404": {
						"description": "CV not found"
					},
					"500": {
						"description": "Internal server error"
					}
				}
			},
			"delete": {
				"summary": "Delete a CV by id",
				"parameters": [
					{
						"in": "path",
						"name": "id",
						"required": true,
						"schema": {
							"type": "string"
						},
						"description": "The id of the CV to delete."
					}
				],
				"responses": {
					"200": {
						"description": "CV successfully deleted"
					},
					"404": {
						"description": "CV not found"
					},
					"500": {
						"description": "Internal server error"
					}
				}
			}
		},
